{
    "name": "dicr/yii2-google",
    "type": "yii2-extension",
    "license": "MIT",
    "description": "Google API клиент для Yii2",
    "keywords": ["dicr", "yii2", "google"],
    "homepage": "https://gitlab.com/dicr/yii2-google",

    "support": {
        "source": "https://gitlab.com/dicr/yii2-google.git"
    },

    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "http://dicr.org",
            "role": "developer"
        }
    ],

    "autoload": {
        "psr-4": {
            "dicr\\google\\": "src/"
        }
    },

    "require": {
        "php": "~7.4 || ~8.0.1",

        "yiisoft/yii2": "~2.0",

        "google/apiclient": "~2.9"
    },

    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },

    "repositories": [
        {"type": "composer", "url": "https://asset-packagist.org"}
    ]
}
